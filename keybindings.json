[
	{ "command": "-inlineChat.holdForSpeech"            , "key": "ctrl+i",
	     "when":     "hasSpeechProvider && inlineChatVisible && textInputFocus" },
	{ "command": "-inlineChat.start"                    , "key": "ctrl+i",
	     "when":     "editorFocus && inlineChatHasEditsAgent && inlineChatPossible && !editorReadonly && !editorSimpleInput || editorFocus && inlineChatHasProvider && inlineChatPossible && !editorReadonly && !editorSimpleInput" },
	{ "command": "-inlineChat.startWithCurrentLine"     , "key": "ctrl+i",
	     "when":     "inlineChatHasProvider && inlineChatShowingHint && !editorReadonly && !inlineChatVisible" },
	{ "command": "-inlineChat.startWithCurrentLine"     , "key": "ctrl+k i",
	     "when":     "inlineChatHasProvider && !editorReadonly && !inlineChatVisible" },
	{ "command": "-inlineChat2.reveal"                  , "key": "ctrl+i",
	     "when":     "inlineChatHasEditsAgent && !chatEdits.isGlobalEditingSession && chatEdits.requestCount >= 1" },
	{ "command": "-inlineChat2.stop"                    , "key": "ctrl+i",
	     "when":     "inlineChatHasEditsAgent && inlineChatVisible && chatEdits.requestCount == '0'" },
	{ "command": "-notebook.cell.chat.start"            , "key": "ctrl+k i",
	     "when":     "config.notebook.experimental.cellChat && notebookChatAgentRegistered && notebookEditable && notebookEditorFocused && !inputFocus || config.notebook.experimental.generate && notebookChatAgentRegistered && notebookEditable && notebookEditorFocused && !inputFocus" },
	{ "command": "-workbench.action.terminal.chat.start", "key": "ctrl+i",
	     "when":     "chatIsEnabled && terminalChatAgentRegistered && terminalFocusInAny && terminalHasBeenCreated || chatIsEnabled && terminalChatAgentRegistered && terminalFocusInAny && terminalProcessSupported" },

	{ "command": "-markdown.showPreview"                                       , "key": "ctrl+shift+v",
	     "when":     "!notebookEditorFocused && editorLangId == 'markdown'"                                   },
	{ "command": "-workbench.action.closeAllEditors"                           , "key": "ctrl+k ctrl+w"       },
	{ "command": "-workbench.action.closeAllGroups"                            , "key": "ctrl+k ctrl+shift+w" },
	{ "command": "-workbench.action.closeEditorsInGroup"                       , "key": "ctrl+k w"            },
	{ "command": "-workbench.action.closeFolder"                               , "key": "ctrl+k f",
	     "when":     "emptyWorkspaceSupport && workbenchState != 'empty'"                                     },
	{ "command": "-workbench.action.closeGroup"                                , "key": "ctrl+w",
	     "when":     "activeEditorGroupEmpty && multipleEditorGroups"                                         },
	{ "command": "-workbench.action.closeUnmodifiedEditors"                    , "key": "ctrl+k u"            },
	{ "command": "-workbench.action.copyEditorToNewWindow"                     , "key": "ctrl+k o"            },
	{ "command": "-workbench.action.files.copyPathOfActiveFile"                , "key": "ctrl+k p"            },
	{ "command": "-workbench.action.quickOpenLeastRecentlyUsedEditorInGroup"   , "key": "ctrl+shift+tab",
	     "when":     "!activeEditorGroupEmpty"                                                                },
	{ "command": "-workbench.action.quickOpenNavigateNextInEditorPicker"       , "key": "ctrl+tab",
	     "when":     "inEditorsPicker && inQuickOpen"                                                         },
	{ "command": "-workbench.action.quickOpenNavigatePreviousInEditorPicker"   , "key": "ctrl+shift+tab",
	     "when":     "inEditorsPicker && inQuickOpen"                                                         },
	{ "command": "-workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup", "key": "ctrl+tab",
	     "when":     "!activeEditorGroupEmpty"                                                                },
	{ "command": "-workbench.files.action.compareWithSaved"                    , "key": "ctrl+k d"            },

	{ "command": "breadcrumbs.toggle"                          , "key": "ctrl+k ;"       },
	{ "command": "hideSuggestWidget"                           , "key": "ctrl+i",
	     "when":     "suggestWidgetVisible && suggestWidgetHasFocusedSuggestion"         },
	{ "command": "selectNextSuggestion"                        , "key": "j",
	     "when":     "suggestWidgetHasFocusedSuggestion"                                 },
	{ "command": "selectPrevSuggestion"                        , "key": "k",
	     "when":     "suggestWidgetHasFocusedSuggestion"                                 },
	{ "command": "workbench.action.closeActiveEditor"          , "key": "ctrl+k q"       },
	{ "command": "workbench.action.closeEditorsAndGroup"       , "key": "ctrl+k alt+w"   },
	{ "command": "workbench.action.files.newUntitledFile"      , "key": "ctrl+k n"       },
	{ "command": "workbench.action.focusAboveGroup"            , "key": "ctrl+k k"       },
	{ "command": "workbench.action.focusBelowGroup"            , "key": "ctrl+k j"       },
	{ "command": "workbench.action.focusLeftGroup"             , "key": "ctrl+k h"       },
	{ "command": "workbench.action.focusRightGroup"            , "key": "ctrl+k l"       },
	{ "command": "workbench.action.moveEditorToAboveGroup"     , "key": "ctrl+k shift+k" },
	{ "command": "workbench.action.moveEditorToBelowGroup"     , "key": "ctrl+k shift+j" },
	{ "command": "workbench.action.moveEditorToLeftGroup"      , "key": "ctrl+k shift+h" },
	{ "command": "workbench.action.moveEditorToRightGroup"     , "key": "ctrl+k shift+l" },
	{ "command": "workbench.action.newEmptyEditorWindow"       , "key": "ctrl+k o"       },
	{ "command": "workbench.action.newGroupAbove"              , "key": "ctrl+k alt+k"   },
	{ "command": "workbench.action.newGroupBelow"              , "key": "ctrl+k alt+j"   },
	{ "command": "workbench.action.newGroupLeft"               , "key": "ctrl+k alt+h"   },
	{ "command": "workbench.action.newGroupRight"              , "key": "ctrl+k alt+l"   },
	{ "command": "workbench.action.nextEditorInGroup"          , "key": "ctrl+tab"       },
	{ "command": "workbench.action.openActiveLogOutputFile"    , "key": "ctrl+k shift+u" },
	{ "command": "workbench.action.previousEditorInGroup"      , "key": "ctrl+shift+tab" },
	{ "command": "workbench.action.quickOpen"                  , "key": "ctrl+k p"       },
	{ "command": "workbench.action.showOutputChannels"         , "key": "ctrl+k u"       },
	{ "command": "workbench.action.tasks.test"                 , "key": "ctrl+k f"       },
	{ "command": "workbench.action.terminal.moveToEditor"      , "key": "ctrl+k `"       },
	{ "command": "workbench.action.terminal.newLocal"          , "key": "ctrl+alt+`"     },
	{ "command": "workbench.action.toggleActivityBarVisibility", "key": "ctrl+k a"       },
	{ "command": "workbench.action.togglePanel"                , "key": "ctrl+k d"       },
	{ "command": "workbench.action.toggleSidebarVisibility"    , "key": "ctrl+k b"       },
	{ "command": "workbench.action.toggleStatusbarVisibility"  , "key": "ctrl+k s"       }
]
